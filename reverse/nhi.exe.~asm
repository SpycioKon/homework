.model small
.stack 100h
.data
tbao db "Nhap chuoi di: $"
tbao1 db "Chuoi da chuyen doi: $"
.code

main proc
mov ax,@data
mov ds,ax

mov ah,9
lea dx,tbao
int 21h

xor cx,cx
mov ah,1
int 21h
while_:
cmp al,0Dh
je end_
push ax
inc cx
int 21h
jmp while_
end_:
mov ah,2
mov dl,0dh
int 21h
mov ah,2
mov dl,0ah
int 21h
mov ah,9
lea dx,tbao1
int 21h
loop_:
pop dx
mov ah,2
int 21h
loop loop_
main endp
end main



